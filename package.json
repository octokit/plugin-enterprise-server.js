{
  "name": "@octokit/plugin-enterprise-server",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "type": "module",
  "description": "Octokit plugin for GitHub Enterprise Server REST APIs",
  "scripts": {
    "build": "node scripts/build.mjs && tsc -p tsconfig.json",
    "lint": "prettier --check '{src,test,scripts}/**' '!scripts/update-endpoints/generated/*' README.md package.json",
    "lint:fix": "prettier --write '{src,test,scripts}/**' '!scripts/update-endpoints/generated/*' README.md package.json",
    "pretest": "npm run -s lint",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest --coverage",
    "update-endpoints": "npm-run-all update-endpoints:*",
    "update-endpoints:fetch-json": "node scripts/update-endpoints/fetch-json.js",
    "update-endpoints:code": "node scripts/update-endpoints/code.js"
  },
  "repository": "github:octokit/plugin-enterprise-server.js",
  "keywords": [
    "octokit",
    "github",
    "api",
    "rest",
    "plugin",
    "enterprise"
  ],
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "license": "MIT",
  "dependencies": {
    "@octokit/types": "^13.6.0"
  },
  "peerDependencies": {
    "@octokit/core": ">=6"
  },
  "devDependencies": {
    "@octokit/core": "^6.0.0",
    "@octokit/tsconfig": "^3.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "esbuild": "^0.24.0",
    "fetch-mock": "^11.0.0",
    "github-enterprise-server-versions": "^2.0.1",
    "github-openapi-graphql-query": "^4.4.0",
    "glob": "^10.2.7",
    "jest": "^29.0.0",
    "npm-run-all2": "^6.0.0",
    "prettier": "3.3.3",
    "semantic-release-plugin-update-version-in-files": "^1.0.0",
    "sort-keys": "^5.1.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "test/tsconfig.test.json",
          "useESM": true
        }
      ]
    },
    "moduleNameMapper": {
      "^(.+)\\.jsx?$": "$1"
    }
  },
  "release": {
    "branches": [
      "+([0-9]).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./pkg"
        }
      ],
      [
        "semantic-release-plugin-update-version-in-files",
        {
          "files": [
            "pkg/dist-web/*",
            "pkg/dist-node/*",
            "pkg/*/version.*"
          ]
        }
      ]
    ]
  },
  "engines": {
    "node": ">= 18"
  }
}
